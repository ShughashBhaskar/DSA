class Main {
    public static void main(String[] args) {
// int[] arr = {1,2,3,2,1};
int[] arr = {1,2,3,4,5};
int mid=arr.length/2;

int flag=0;
   for(int i=0;i<mid;i++){
      if(arr[i]!=arr[arr.length-1-i]){
          flag++;
          break;
      }
    }
    if(flag==1){
    System.out.print("false");}else{
        System.out.print("true");
    }
}}
